# ��� .env - Environment variables
# This file contains environment variables for the DevElevate server.
# Application Port
PORT=4000

# MongoDB connection string
# Use your own MongoDB connection URL to connect to your own database
# Example: mongodb+srv://<username>:<password>@<cluster-url>.mongodb.net/<your-database-name>?retryWrites=true&w=majority
MONGO_URI=mongodb+srv://<username>:<password>@<cluster-url>.mongodb.net/<your-database-name>?retryWrites=true&w=majority

# After verifying it works locally, push your code.
# The admin system will automatically check and connect the database.

# JWT secret for authentication
JWT_SECRET=<your_jwt_secret>


























